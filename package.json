{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "@types/classnames": "^2.2.10",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^16.9.35",
    "@types/styled-components": "^5.0.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.1",
    "antd": "^4.2.5",
    "axios": "^0.19.2",
    "basscss": "^8.1.0",
    "classnames": "^2.2.6",
    "dva-logger": "^1.0.0",
    "js-cookie": "^2.2.1",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-simple-line-icons": "^1.0.8",
    "styled-components": "^5.0.1",
    "umi": "3.1.2",
    "use-deep-compare-effect": "^1.3.1",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "husky": "^4.2.3"
  }
}
